<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bluemobi.pro.entity.Image">
	
	<!-- 新增图片 -->
	<insert id="insert" parameterType="com.bluemobi.pro.entity.Image">
		INSERT INTO
			image
		(
			`path`,
			`feekback_id`
		)
		VALUES
		(
			#{path},
			#{feekbackId}
		)
	</insert>
</mapper> 